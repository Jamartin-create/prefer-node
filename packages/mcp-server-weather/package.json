    "dotenv": "^16.4.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "sdk": "link:@types/modelcontextprotocol/sdk",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.4.5"
  }
}